<%# -*- encoding : utf-8 -*- %>
<!DOCTYPE html>
<html>
    <%= erb :head, :locals => {:current_feed_id => current_feed_id} %>
    <body>
        <% current_category = nil if defined?(current_category).nil? %>
        <%= erb :feedlist, :locals => {:feeds => feeds, :current_feed_id => current_feed_id, :current_category => current_category } %>

        <%= yield %>
    </body>
</html>
