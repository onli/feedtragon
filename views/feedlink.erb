<%# -*- encoding : utf-8 -*- %>
 <li class="feedlink<%= feed.id.to_i == current_feed_id.to_i ? ' current' : ''%>" id="feed_<%= feed.id %>"><a href="<%= url_for '/' + feed.id.to_s %>"><%= truncate(feed.name, 35, "...") %></a>
    <form class="unsubscribe" method="POST" action="<%= url_for '/' + feed.id.to_s + '/unsubscribe' %>">
        <button type="submit" title="unsubscribe">x</button>
    </form>
 </li>